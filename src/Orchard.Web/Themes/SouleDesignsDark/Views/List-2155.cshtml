@using Orchard.ContentManagement;
@{
    // This is the 2017 sketchbook only, used by a blog post which is why it is 
    // queried out separately 
    ViewBag.Subtitle = "2017";
    // Turn on auto load for this year of sketchbookery
    ViewBag.LoadMore = false;
    var model = ((List<dynamic>)Model.Items).Select(item => (ContentItem)item.ContentItem).ToList();
    Html.RenderPartial("SketchbookPartial", model);
}
